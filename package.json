{
  "name": "otrust",
  "version": "0.1.0",
  "private": true,
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!./src/utils/device.js",
      "!./src/theme/colors.js",
      "!./src/components/Chart/Style.js"
    ],
    "coverageReporters": [
      "text-summary",
      "json",
      "html",
      "lcovonly"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "dependencies": {
    "@apollo/client": "^3.4.11",
    "@cosmjs/launchpad": "^0.26.0",
